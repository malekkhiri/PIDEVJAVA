<AnchorPane id="AnchorPane" prefHeight="437.0" prefWidth="740.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.DashboardController">
   <children>
      <TabPane layoutY="28.0" prefHeight="407.0" prefWidth="646.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Annonces">
            <content>
              <AnchorPane fx:id="tabannonce" minHeight="0.0" minWidth="0.0" prefHeight="457.0" prefWidth="646.0">
                     <children>
                        <TableView fx:id="tableannonce" layoutX="-2.0" prefHeight="379.0" prefWidth="648.0">
                          <columns>
                            <TableColumn fx:id="titre" prefWidth="163.0" text="Titre" />
                            <TableColumn fx:id="description" prefWidth="165.0" text="Description" />
                              <TableColumn fx:id="prix" prefWidth="93.0" text="Prix" />
                              <TableColumn fx:id="ville" prefWidth="89.0" text="Ville" />
                              <TableColumn fx:id="region" prefWidth="135.0" text="Region" />
                              <TableColumn fx:id="annonceid" editable="false" minWidth="0.0" prefWidth="0.0" sortable="false" text="id" visible="false" />
                          </columns>
                        </TableView>
                     </children></AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
      <MenuBar layoutY="2.0" prefHeight="28.0" prefWidth="774.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <Button fx:id="btnajout" layoutX="646.0" layoutY="96.0" mnemonicParsing="false" onAction="#addAnnonce" prefHeight="73.0" prefWidth="129.0" text="Ajouter Anoonce" />
      <Button fx:id="btnfavoris" layoutX="646.0" layoutY="169.0" mnemonicParsing="false" onAction="#showWishList" prefHeight="73.0" prefWidth="129.0" text="Mes Favoris" />
      <Button fx:id="btnmesannonce" layoutX="646.0" layoutY="323.0" mnemonicParsing="false" onAction="#showAnnonces" prefHeight="81.0" prefWidth="129.0" text="Tous les Annonces" />
      <Button fx:id="btnannonce" layoutX="646.0" layoutY="242.0" mnemonicParsing="false" onAction="#showMyAnnoces" prefHeight="81.0" prefWidth="129.0" text="Mes annonces" />
      <Button fx:id="refresh" layoutX="647.0" layoutY="404.0" mnemonicParsing="false" onAction="#reloadPage" prefHeight="31.0" prefWidth="129.0" text="Refresh" />
      <JFXButton fx:id="voirBtn" layoutX="646.0" layoutY="70.0" onAction="#showAnnonce" prefHeight="7.0" prefWidth="129.0" text="Voir" />
      <Label fx:id="idannoncelab" disable="true" layoutX="645.0" layoutY="399.0" prefHeight="17.0" prefWidth="0.0" visible="false" />
      <JFXTextField fx:id="filterField" layoutX="648.0" layoutY="30.0" prefHeight="25.0" prefWidth="122.0" promptText="  Chercher" />
   </children>
